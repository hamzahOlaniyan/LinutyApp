{
   "cli": {
      "version": ">= 16.17.4",
      "appVersionSource": "remote"
   },
   "build": {
      "development": {
         "developmentClient": true,
         "distribution": "internal",
         "autoIncrement": true,
         "channel": "development"
      },
      "preview": {
         "distribution": "internal",
         "android": {
            "buildType": "apk",
            "gradleCommand": ":app:assembleRelease"
         },
         "autoIncrement": true,
         "channel": "preview"
      },
      "production": {
         "autoIncrement": true,
         "channel": "production"
      }
   },
   "submit": {
      "production": {}
   }
}
